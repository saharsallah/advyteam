
@{
    ViewBag.Title = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Supression du Questionnaire</title>
</head>
<body>

    



        <div class="col-md-9">

            <div class="dashboard-list-box fl-wrap">
                <div class="dashboard-header fl-wrap">
                    <h3>Supression du Questionnaire</h3>
                </div>
                <!-- dashboard-list end-->

                <div class="dashboard-list">
                    <div class="dashboard-message">

                        <div class="col-xl-8">
                            <h3>Are you sure you want to delete this?</h3>
                            <div>

                                <br /><br />

                                <div class="col-lg-12">

                                    @using (Html.BeginForm())
                                    {
                                        @Html.AntiForgeryToken()

                                        <div class="form-actions no-color">
                                            <input type="submit" value="Delete" class="new-dashboard-item" />

                                            <button class="new-dashboard-item"> @Html.ActionLink("Cancel", "Index")</button>
                                        </div>
                                    }
                                </div>

                             

                            </div>
                        </div>
                    </div>
                </div>


            </div>

        </div>
    



</body>
</html>